{
    "name": "aknevrnky/perfect-panel",
    "version": "0.1.1",
    "description": "Perfect panel API SDK",
    "type": "library",
    "license": "MIT",
    "autoload": {
        "psr-4": {
            "AknEvrnky\\PerfectPanel\\": "src/"
        }
    },
    "autoload-dev": {
        "psr-4": {
            "Tests\\": "tests/"
        }
    },
    "authors": [
        {
            "name": "aknEvrnky"
        }
    ],
    "minimum-stability": "stable",
    "require": {
        "php": ">=8.0",
        "guzzlehttp/guzzle": "^7.7",
        "illuminate/support": "^10.15"
    },
    "require-dev": {
        "pestphp/pest": "^2.9",
        "phpstan/phpstan": "^1.10"
    },
    "config": {
        "allow-plugins": {
            "pestphp/pest-plugin": true
        }
    },
    "scripts": {
        "test": "vendor/bin/pest",
        "test-coverage": "vendor/bin/pest --coverage",
        "phpstan": "phpstan analyse"
    }
}
